pipeline {
    agent any

    stages {
        stage('git checkout') {
            steps {
                echo 'this is git checkout phase'
            }
        }
         stage('build') {
            steps {
                echo 'this is build phase'
            }
        }
         stage('test') {
            steps {
                echo 'this is testing phase'
            }
        }
         stage('deploy') {
            steps {
                echo 'this is deploy phase'
            }
        }
    }
}

